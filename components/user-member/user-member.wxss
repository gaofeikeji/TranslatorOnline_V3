/* components/user-member/user-member.wxss */
.container {
  width: 100%;
}
.open-vip-box {
  box-sizing: border-box;
  width: 100%;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: rgba(0, 0, 0, 0.3);
  padding: 26rpx 34rpx 26rpx 26rpx;
  border-radius: 10rpx;

  font-size: 32rpx;
}
.open-vip-box image {
  width: 68rpx;
  height: 68rpx;
  margin: 0 10rpx 0 0;
}
.normal-user {
  color: rgba(255, 255, 255, 1);
}
.open-bnts {
  color: rgba(255, 255, 255, 1); 
  background: rgba(255, 255, 255, 0.31);
}
.open-bnts,
.renewal-bnts {
  border-radius: 8rpx;
  margin-left: auto;
  width: 200rpx;
  height: 68rpx;
  font-size: 32rpx;
  font-weight: 700;
  display: flex;
  align-items: center;
  justify-content: center;
}
.renewal-vip-box {
  box-sizing: border-box;
  width: 100%;
  height: 140rpx;
  display: flex;
  align-items:center;
  justify-content: space-between;
  background: rgba(250, 212, 135, 1);
  padding: 26rpx 34rpx 26rpx 26rpx;
  border-radius: 10rpx;
  border: 1px solid rgba(0, 0, 0, 0.08);
  box-shadow: 0px 0px 10px  rgba(0, 0, 0, 0.02);
}
.renewal-vip-box image {
  width: 68rpx;
  height: 68rpx;
  margin: 0 10rpx 0 0;
}
.expired {
  display: flex;
  flex-direction: column;
  align-items: flex-start;

}
.vip-user {
  font-size: 32rpx;
  font-weight: 400;
  line-height: 46rpx;
  color: rgba(140, 94, 0, 1);
}
.expired-time {
  font-size: 28rpx;
  font-weight: 400;
  line-height: 40.54rpx; 
  color: rgba(140, 94, 0, 0.6);
}
.renewal-bnts {
  background: rgba(255, 255, 255, 0.31);
  color: rgba(140, 94, 0, 1);
}
.open-bnts:active,
.renewal-bnts:active {
  opacity: 0.7;
}

.user_btn{
  border: none;
  padding: 0;
  margin:0;
  /* font: inherit; */
}
.user_btn::after{
  border: none;
}
.btn_back{
  display: flex;
  flex-direction: row;
  align-items: center;
  
}