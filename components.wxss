/* +++ 组件样式 +++ */

/* 按钮 */
/* + 添加其他字段 */
.btn__plus {
  display: flex;
  align-items: center;
}
.btn__plus .text {
  margin-left: var(--xyui-mg-8);
  font-size: var(--xyui-FONT-SIZE-16);
  color: var(--xyui-FONT-COLOR-2);
}

/* + 添加人员名单 带边框 */
.btn__plus-border {
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: var(--xyui-BG-white-5);
  border: var(--border-size-2) solid var(--xyui-FONT-COLOR-8);
  padding: 22rpx 52rpx;
  border-radius: var(--border-radius-4);
}
.btn__plus-border .text {
  margin-left: var(--xyui-mg-8);
  font-size: var(--xyui-FONT-SIZE-16);
  color: var(--xyui-FONT-COLOR-2);
}

/* 强调按钮 */
.btn__focus button {
  color: var(--xyui-BG-white-0);
  background-color: var(--xyui-BG-0);
}

/* 弱化按钮 */
.btn__weaken button {
  color: var(--xyui-font-color-blue);
  background-color: var(--xyui-BG-white-2);
}

/* 失效按钮 */
.btn__disabled button {
  color: var(--xyui-FONT-COLOR-8);
  background-color: var(--xyui-BG-white-2);
}

/* 警示按钮 */
.btn__warn button {
  color: var(--xyui-warn-1);
  background-color: var(--xyui-BG-white-2);
}

/* 播放/暂停按钮 */
.btn__play .start {
  background-color: var(--xyui-BG-0);
}
.btn__play .pause {
  background-color: var(--xyui-stop-1);
}
.btn__play text {
  margin-left: var(--xyui-mg-6);
}
.btn__play button {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 360rpx;
  height: 80rpx;
  color: var(--xyui-BG-white-0);
  font-weight: bold;
  border-radius: 200rpx;
  border: var(--border-size-2) solid var(--xyui-FONT-COLOR-12);
  box-shadow: 0 0 20rpx var(--xyui-FONT-COLOR-11);
}

/* 计数 */
.count__container {
  display: flex;
  align-items: center;
}
.count__container .plus,
.count__container .sub {
  width: 60rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0;
  padding: 0;
  background-color: var(--xyui-FONT-COLOR-11);
  border-radius: var(--border-radius-2);
}
.count__container .num {
  margin: 0 10rpx;
  padding: 0 var(--xyui-pd-20);
  background-color: var(--xyui-BG-4);
  font-size: var(--xyui-FONT-SIZE-16);
  color: var(--xyui-BG-1);
  height: 60rpx;
  line-height: 60rpx;
  border-radius: var(--border-radius-2);
}


/* 格子 */
.box_lattice {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
}
.box_lattice .list_item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 150rpx;
  border: var(--border-size-2) solid var(--xyui-FONT-COLOR-10);
}
.box_lattice .list_item text {
  margin-top: 8rpx;
  font-size: var(--xyui-FONT-SIZE-12);
  color: var(--xyui-FONT-COLOR-3);
}

/* 大列表 */
.large_list {
  padding-left: var(--xyui-pd-15);
}
.large_list-item {
  display: flex;
  align-items: center;
  box-sizing: border-box;
  height: 110rpx;
  line-height: 110rpx;
}
.large_list-item .large_opt {
  box-sizing: border-box;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-left: var(--xyui-mg-10);
  padding-right: 26rpx;
  border-bottom: var(--border-size-2) solid var(--xyui-FONT-COLOR-10);
}
.large_list:last-child .large_opt {
  border-bottom: none;
}
.large_list-item .large_opt text {
  color: var(--xyui-FONT-COLOR-1);
  font-size: var(--xyui-FONT-SIZE-16);
}

/* 中列表 */
.mid_list {
  padding-left: var(--xyui-pd-15);
}
.mid_list_container {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: var(--border-size-2) solid var(--xyui-FONT-COLOR-10);
}
.mid_list:last-child .mid_list_container {
  border-bottom: none;
}
.mid_list .mid_list-l {
  display: flex;
  align-items: center;
}
.mid_list .mid_list-l text {
  margin-right: var(--xyui-mg-8);
  font-size: var(--xyui-FONT-SIZE-16);
  color: var(--xyui-FONT-COLOR-3);
}
.mid_list .mid_list-r {
  display: flex;
  align-items: center;
  padding-right: 26rpx;
}
.mid_list .mid_list-r text {
  color: var(--xyui-FONT-COLOR-1);
  font-size: var(--xyui-FONT-SIZE-16);
  margin-right: var(--xyui-mg-8);
}

/* 定位 */
.location__container {
  display: flex;
  align-items: center;
  background-color: var(--xyui-FONT-COLOR-11);
  border-radius: var(--border-radius-2);
  border: var(--border-size-1) solid var(--xyui-FONT-COLOR-10);
  height: 110rpx;
  padding: 0 var(--xyui-pd-10);
}
.location__container .location-r {
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: var(--xyui-mg-10);
}
.location__container .location-r .title {
  font-size: var(--xyui-FONT-SIZE-14);
  color: var(--xyui-FONT-COLOR-1);
}
.location__container .location-r .desc {
  font-size: var(--xyui-FONT-SIZE-12);
  color: var(--xyui-FONT-COLOR-5);
}

/* 快速创建模板 */
.tpl__container {
  display: flex;
  align-items: center;
  gap: 20rpx;
}
.tpl__container .tpl_item {
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: var(--xyui-BG-white-0);
  border-radius: var(--border-radius-5);
  border: var(--border-size-2) solid var(--xyui-FONT-COLOR-10);
  padding: 0 var(--xyui-pd-15) 0 22rpx;
  height: 170rpx;
  width: 344rpx;
}
.tpl__container .tpl_item .tpl_item-r {
  display: flex;
  flex-direction: column;
  margin-left: 10rpx;
}
.tpl__container .tpl_item .tpl_item-r .title {
  font-size: var(--xyui-FONT-SIZE-18);
  font-weight: bold;
}
.tpl__container .tpl_item .tpl_item-r .desc {
  font-size: var(--xyui-FONT-SIZE-14);
}
.tpl__container .tpl_item.current {
  background-color: var(--xyui-BG-0);
}
.tpl__container .tpl_item.current .tpl_item-r .title {
  color: var(--xyui-BG-white-4);
}
.tpl__container .tpl_item.current .tpl_item-r .desc {
  color: var(--xyui-BG-white-4);
}

/* tab切换 */
/* 普通模式 / 热身模式 */
.tab__select {
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-shadow: 0 0 10rpx var(--xyui-FONT-COLOR-11);
}
.tab__select .mode {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 50%;
  height: 66rpx;
  border: var(--border-size-2) solid var(--xyui-FONT-COLOR-8);
  font-size: var(--xyui-FONT-SIZE-14);
  color: var(--xyui-FONT-COLOR-3);
  line-height: 36rpx;
  background-color: var(--xyui-BG-white-0);
}
.tab__select .mode:first-child {
  border-radius: var(--border-radius-2) 0 0 var(--border-radius-2);
}
.tab__select .mode:last-child {
  border-radius: 0 var(--border-radius-2) var(--border-radius-2) 0;
}
.tab__select .mode:first-child {
  border-right: none;
}
.tab__select .mode.selected {
  background-color: var(--xyui-BG-4);
  color: var(--xyui-BG-1);
  border: var(--border-size-2) solid var(--xyui-BG-1);
}

/* 字段名称 通用盒子 */
.box__container {
  background-color: var(--xyui-BG-white-0);
  border: var(--border-size-2) solid var(--xyui-FONT-COLOR-12);
  border-radius: var(--border-radius-5);
  box-shadow: 0px 0px 20rpx var(--xyui-FONT-COLOR-11);
}
.box__container .tit {
  color: var(--xyui-FONT-COLOR-6);
  font-size: var(--xyui-FONT-SIZE-16);
  height: 110rpx;
  line-height: 110rpx;
  padding-left: var(--xyui-pd-15);
  border-bottom: var(--border-size-2) solid var(--xyui-FONT-COLOR-12);
}
.box__container .content {
  padding: var(--xyui-mg-15);
}

/* tab切换 近期热点 */
.tab__toggle {
  display: flex;
  align-items: top;
  gap: 50rpx;
  font-size: var(--xyui-FONT-SIZE-14);
  padding: var(--xyui-pd-20);
}
.tab__toggle .tb {
  width: fit-content;
}
.tab__toggle .tb .text {
  color: var(--xyui-FONT-COLOR-5);
}
.tab__toggle .tb.current .text {
  color: var(--xyui-FONT-COLOR-0);
}
.tab__toggle .tb.current .line {
  width: 85%;
  height: 6rpx;
  margin: 6rpx auto 0;
  background-color: var(--xyui-BG-0);
  border-radius: 100rpx;
}

/* 选项条 */
.option {
  box-sizing: border-box;
  border: var(--border-size-2) solid var(--xyui-FONT-COLOR-12);
  background-color: var(--xyui-BG-white-0);
  border-radius: var(--border-radius-5);
  box-shadow: 0px 0px 20rpx var(--xyui-FONT-COLOR-11);
  padding: 0 var(--xyui-pd-15);
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.option .title {
  font-size: var(--xyui-FONT-SIZE-16);
  color: var(--xyui-FONT-COLOR-3);
}
.option .function {
  display: flex;
  align-items: center;
  flex-direction: row;
}
.option .function text {
  color: var(--xyui-FONT-COLOR-1);
  font-size: var(--xyui-FONT-SIZE-16);
  margin-right: var(--xyui-mg-8);
}
.hover:active {
  background-color: var(--xyui-FONT-COLOR-11);
}

/* 成功状态 */
.status_head {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin: 70rpx auto 90rpx;
}
.status_head text {
  margin-top: 16rpx;
  font-size: var(--xyui-FONT-SIZE-20);
  color: var(--xyui-FONT-COLOR-0);
}
.status_bottom {
  height: 140rpx;
  line-height: 140rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-top: var(--border-size-2) solid var(--xyui-FONT-COLOR-10);
}
.status_bottom .link {
  color: var(--xyui-FONT-COLOR-5);
  font-size: var(--xyui-FONT-SIZE-16);
}
.status_bottom .link text {
  color: var(--xyui-font-color-blue);
}

/* 图文列表 */
.teletext_item {
  display: flex;
  align-items: center;
  margin-left: var(--xyui-mg-10);
}
.teletext_item image {
  width: 160rpx;
  height: 128rpx;
  border-radius: var(--border-radius-4);
}
.teletext_item .content {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: center;
  flex: 1;
  box-sizing: border-box;
  padding: 0;
  margin: 0 0 0 var(--xyui-pd-10);
  border-bottom: var(--border-size-2) solid var(--xyui-FONT-COLOR-10);
  height: 180rpx;
}
.teletext_item:last-child .content {
  border-bottom: none;
}
.teletext_item .content .title {
  color: var(--xyui-FONT-COLOR-1);
  font-size: var(--xyui-FONT-SIZE-16);
}
.teletext_item .content .desc {
  color: var(--xyui-FONT-COLOR-5);
  font-size: var(--xyui-FONT-SIZE-14);
  line-height: 1.2;
  white-space: wrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

/* 图文混排 */
.list_container {
  display: flex;
  align-items: center;
  padding: var(--xyui-pd-10);
}
.list_container image {
  width: 90rpx;
  height: 90rpx;
  border-radius: var(--border-radius-4);
}
.list_container .list_content {
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: var(--xyui-mg-15);
}
.list_container .list_content .list_title {
  font-size: var(--xyui-FONT-SIZE-16);
  color: var(--xyui-FONT-COLOR-1);
}
.list_container .list_content .list_date {
  color: var(--xyui-FONT-COLOR-6);
  font-size: var(--xyui-FONT-SIZE-12);
}
.list_container .state_icon {
  margin-left: auto;
}
.list_container .state_icon .label {
  font-size: var(--xyui-FONT-SIZE-12);
  color: var(--xyui-warn-1);
  padding: 2rpx var(--xyui-pd-8);
  border-radius: var(--border-radius-2);
  background-color: var(--xyui-warn-2);
  border: var(--border-size-2) solid var(--xyui-warn-1);
  height: 36rpx;
  line-height: 36rpx;
}

/* 打卡填写内容 */
.form_header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 90rpx;
  padding: 0 var(--xyui-pd-15);
  border-bottom: var(--border-size-2) solid var(--xyui-FONT-COLOR-10);
}
.form_header text {
  font-size: var(--xyui-FONT-SIZE-16);
  color: var(--xyui-FONT-COLOR-3);
}
.form_list {
  display: flex;
  align-items: center;
  height: 100rpx;
  padding: 0 0 0 var(--xyui-pd-15);
}
.form_list .form_input {
  display: flex;
  align-items: center;
  justify-content: space-between;
  flex: 1;
  box-sizing: border-box;
  height: 100rpx;
  margin-left: var(--xyui-mg-10);
  border-bottom: var(--border-size-2) solid var(--xyui-FONT-COLOR-10);
  padding: 0 var(--xyui-pd-15) 0 0;
}
.form_list .form_input input {
  flex: 1;
}
.form_list .form_input .require {
  background-color: var(--xyui-BG-white-2);
  font-size: var(--xyui-FONT-SIZE-14);
  padding: 0 var(--xyui-pd-8);
  border-radius: var(--border-radius-2);
  color: var(--xyui-FONT-COLOR-6);
}
.box__container .form_add {
  height: 100rpx;
}

/* 警示颜色 */
.warn {
  color: var(--xyui-warn-1) !important;
}

/* 日历 */
.calendar__container {
  border: var(--border-size-2) solid var(--xyui-FONT-COLOR-12);
  border-radius: var(--border-radius-5);
  box-shadow: 0px 0px 20rpx var(--xyui-FONT-COLOR-11);
  padding: var(--xyui-pd-15) var(--xyui-pd-15);
}
.calendar__container .cal_header {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.calendar__container .cal_header .cal_selected {
  display: flex;
  align-items: center;
}
.calendar__container .cal_header .cal_selected .date {
  color: var(--xyui-FONT-COLOR-1);
  font-size: var(--xyui-FONT-SIZE-16);
  margin-right: var(--xyui-mg-10);
}
.calendar__container .cal_header .cal_btn {
  width: auto;
  height: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: var(--xyui-FONT-SIZE-14);
  margin: 0;
  color: var(--xyui-BG-1);
  background-color: var(--xyui-BG-4);
  padding: 2rpx var(--xyui-pd-10);
  border-radius: var(--border-radius-2);
  font-weight: normal;
}
.calendar__container .week {
  margin-top: var(--xyui-mg-15);
  display: grid;
  justify-content: center;
  grid-template-columns: repeat(7, 74rpx);
  gap: 20rpx;
}
.calendar__container .week > text {
  color: var(--xyui-FONT-COLOR-3);
  font-size: var(--xyui-FONT-SIZE-14);
  text-align: center;
}
.calendar__container .day {
  margin-top: var(--xyui-mg-15);
  display: grid;
  grid-template-columns: repeat(7, 74rpx);
  gap: 20rpx;
  align-items: center;
  justify-content: center;
  grid-template-rows: 74rpx;
}
.calendar__container .day > text {
  position: relative;
  z-index: 99;
  height: 74rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: var(--xyui-FONT-COLOR-10);
  border-radius: var(--border-radius-2);
  color: var(--xyui-FONT-COLOR-4);
  font-size: var(--xyui-FONT-SIZE-14);
}
.calendar__container .day > text.current {
  background-color: var(--xyui-BG-4);
  border: var(--border-size-2) solid var(--xyui-BG-1);
  color: var(--xyui-BG-1);
}
.calendar__container .day > text.empty {
  background-color: var(--xyui-FONT-COLOR-13);
}
.calendar__container .day > text .icon {
  position: absolute;
  top: 0;
  right: 0;
  z-index: 100;
  width: 20rpx;
  height: 20rpx;
}

.dialog__container {
  position: absolute;
  inset: 0;
  z-index: 999;
  width: 100%;
  height: 100vh;
  background-color: var(--xyui-FONT-COLOR-4);
}
/* 分享 */
.export_box,
.share_box {
  position: absolute;
  inset-inline: 0;
  bottom: 0;
  z-index: 1000;
  border-radius: var(--border-radius-4) var(--border-radius-4);
  background-color: var(--xyui-BG-white-0);
  box-shadow: 0px 0px 20rpx var(--xyui-FONT-COLOR-11);
  padding-bottom: calc(constant(safe-area-inset-bottom) + var(--xyui-pd-15));
  padding-bottom: calc(env(safe-area-inset-bottom) + var(--xyui-pd-15));
  animation: show 0.3s ease-in both;
}
.share_box .share_list {
  padding: var(--xyui-pd-20);
  display: flex;
  align-items: center;
  gap: 0 var(--xyui-mg-15);
}
.share_box .share_list .share_item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.share_box .share_list .share_item .icon {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100rpx;
  height: 100rpx;
  border-radius: var(--border-radius-4);
  background-color: var(--xyui-BG-white-2);
}
.share_box .share_list .share_item text {
  margin-top: var(--xyui-mg-8);
  color: var(--xyui-FONT-COLOR-4);
  font-size: var(--xyui-FONT-SIZE-12);
}
.share_box .cancel_btn {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.share_box .share_btn {
  margin: 0;
  padding: 0;
  border: 0;
  line-height: inherit;
  outline: none;
  background-color: transparent;
}
.share_box .share_btn::after {
  border: none;
}
.share_box .cancel_btn .cancel {
  width: 270rpx;
  height: 80rpx;
  background-color: var(--xyui-BG-white-2);
  color: var(--xyui-FONT-COLOR-4);
  font-size: var(--xyui-FONT-SIZE-16);
  border: var(--border-size-2) solid var(--xyui-FONT-COLOR-12);
  box-shadow: 0 0 20rpx var(--xyui-FONT-COLOR-11);
}

/* 导出数据 */
.export_box .title {
  height: 100rpx;
  line-height: 100rpx;
  padding-left: var(--xyui-pd-20);
  font-weight: bold;
  border-bottom: var(--border-size-2) solid var(--xyui-FONT-COLOR-10);
}
.export_box .export_list .list_item .list_item_active {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 100rpx;
  margin-left: var(--xyui-mg-20);
  padding-right: var(--xyui-mg-15);
  border-bottom: var(--border-size-2) solid var(--xyui-FONT-COLOR-10);
}
.export_box .export_list .list_item:last-child .list_item_active {
  border-bottom: none;
}
.export_box .export_list .list_item .datetime {
  font-size: var(--xyui-FONT-SIZE-16);
  color: var(--xyui-FONT-COLOR-1);
}
.export_box .export_list .list_item .date {
  display: flex;
  align-items: center;
}
.export_box .export_list .list_item .date > text {
  font-size: var(--xyui-FONT-SIZE-14);
  margin-right: var(--xyui-pd-8);
}
.export_box .export_btn {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 0 var(--xyui-mg-15);
  margin-top: var(--xyui-mg-15);
}
.export_box .export_btn button {
  width: 270rpx;
  height: 80rpx;
  margin: 0;
  font-size: var(--xyui-FONT-SIZE-16);
  border: var(--border-size-2) solid var(--xyui-FONT-COLOR-12);
  box-shadow: 0 0 20rpx var(--xyui-FONT-COLOR-11);
}
.export_box .export_btn button.cancel {
  color: var(--xyui-FONT-COLOR-4);
  background-color: var(--xyui-BG-white-2);
}
.export_box .export_btn button.done {
  color: var(--xyui-BG-white-0);
  background-color: var(--xyui-BG-1);
}

/* 底部固定按钮 带安全边距 */
.footer_btn {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 990;
  background-color: var(--xyui-BG-white-0);
  box-shadow: 0px 0px 20rpx var(--xyui-FONT-COLOR-11);
}
.footer_btn .submit_btn {
  display: flex;
  align-items: center;
  justify-content: center;
  max-width: 360rpx;
  height: 80rpx;
  margin: 20rpx auto;
  border: var(--border-size-2) solid var(--xyui-FONT-COLOR-9);
  box-shadow: 0px 0px 8rpx var(--xyui-FONT-COLOR-8);
  background-color: var(--xyui-BG-1);
  color: var(--xyui-BG-white-0);
  font-weight: bold;
}

/* 个人主页 */
.user_container {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: var(--xyui-mg-25) auto var(--xyui-mg-20);
}
.user_container .userinfo {
  display: flex;
  align-items: center;
}
.user_container .userinfo .avatar {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 90rpx;
  height: 90rpx;
  border: 6rpx solid rgba(255, 255, 255, 1);
  border-radius: var(--border-radius-5);
  background: rgba(235, 235, 235, 1);
  box-shadow: 0px 0px 20rpx var(--xyui-FONT-COLOR-11);
}
.user_container .userinfo > image {
  width: 90rpx;
  height: 90rpx;
  border-radius: var(--border-radius-5);
  background: rgba(235, 235, 235, 1);
  box-shadow: 0px 0px 20rpx var(--xyui-FONT-COLOR-11);
  border: 6rpx solid rgba(255, 255, 255, 1);
}
.user_container .userinfo text {
  font-size: var(--xyui-FONT-SIZE-16);
  color: var(--xyui-FONT-COLOR-2);
  font-weight: bold;
  margin-left: var(--xyui-mg-15);
}
.user_container .user_edit {
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 16rpx 14rpx 16rpx 24rpx;
}
.user_container .user_edit:active {
  padding: 16rpx 12rpx 16rpx 22rpx;
  border: var(--border-size-2) solid var(--xyui-FONT-COLOR-10);
  border-radius: var(--border-radius-5);
  background-color: var(--xyui-FONT-COLOR-14);
}
.user_container .user_edit text {
  font-size: var(--xyui-FONT-SIZE-16);
  color: var(--xyui-FONT-COLOR-6);
  margin-right: var(--xyui-mg-6);
}

/* 版本 */
.version {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: var(--xyui-FONT-SIZE-14);
  color: var(--xyui-FONT-COLOR-7);
  margin-top: 56rpx;
}

/* 更新用户页面 */
.edit_header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: var(--xyui-mg-20) auto var(--xyui-mg-15);
}
.edit_header .random_avatar {
  display: flex;
  align-items: center;
}
.edit_header .random_avatar text {
  color: var(--xyui-FONT-COLOR-2);
  font-size: var(--xyui-FONT-SIZE-16);
  margin-left: var(--xyui-mg-8);
}
.update_info { 
  padding-left: var(--xyui-pd-15);
}
.update_info .update_nickname {
  height: 110rpx;
  display: flex;
  align-items: center;
  padding-right: var(--xyui-pd-15);
  border-bottom: var(--border-size-2) solid var(--xyui-FONT-COLOR-10);
}
.update_info .update_nickname label {
  color: var(--xyui-FONT-COLOR-1);
  margin-right: var(--xyui-mg-25);
  font-size: var(--xyui-FONT-SIZE-16);
}
.update_info .update_nickname input {
  flex: 1;
  margin-top: -4rpx;
  color: var(--xyui-FONT-COLOR-1);
  font-size: var(--xyui-FONT-SIZE-16);
}
.update_info .update_avatar {
  display: flex;
  align-items: flex-start;
  color: var(--xyui-FONT-COLOR-1);
  font-size: var(--xyui-FONT-SIZE-16);
  margin: var(--xyui-pd-15) auto;
  padding-right: var(--xyui-pd-15);
}
.update_info .update_avatar text {
  margin-right: var(--xyui-mg-25);
}
.update_info .update_avatar image {
  width: 90rpx;
  height: 90rpx;
  border-radius: var(--border-radius-5);
  background: rgba(235, 235, 235, 1);
  box-shadow: 0px 0px 20rpx var(--xyui-FONT-COLOR-11);
}
.update_info .update_avatar .avatar_img {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 90rpx;
  height: 90rpx;
  border-radius: var(--border-radius-5);
  background: rgba(235, 235, 235, 1);
  box-shadow: 0px 0px 20rpx var(--xyui-FONT-COLOR-11);
}
.update_info .update_avatar .setting {
  margin: 0 0 0 auto;
  padding: 0;
  width: 146rpx;
  height: 50rpx;
  color: var(--xyui-BG-1);
  font-size: var(--xyui-FONT-SIZE-14);
  font-weight: normal;
  display: flex;
  align-items: center;
  justify-content: center;
  border: var(--border-size-2) solid var(--xyui-BG-1);
  border-radius: var(--border-radius-2);
  background-color: var(--xyui-BG-4);
  box-shadow: 0px 0px 10rpx var(--xyui-FONT-COLOR-11);
}

/* 广告样式 */
.bottom_ad {
  box-sizing: border-box;
  width: 100%;
  position: fixed;
  padding: 0 20rpx;
  left: 0;
  bottom: 0;
  z-index: 100;
}
.adcontainer {
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
}

/* 动画 */
@keyframes show {
  0% {
    transform: translateY(100%);
  }
  100% {
    transform: translateY(0%);
  }
}