/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  background: #000;
} 
.text-box-notice{
  color: #fff;
}
button.circle-btn{
  width: 300rpx !important;
  border-radius: 40rpx;
  padding: 28rpx 0;
  background: linear-gradient(135deg, rgba(31, 113, 255, 0.8) 0%, rgba(31, 113, 255, 1) 100%);
  color: #fff;
  border: 2px solid #fff;
  box-shadow: -4px 4px 4px -3px rgba(31, 113, 255, 1);
}
